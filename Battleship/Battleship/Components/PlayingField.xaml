                    </DataTrigger>
                    <DataTrigger 
                        Binding="{Binding RelativeSource={RelativeSource AncestorType=local:PlayingField, AncestorLevel=1}, Path=IsEnabled}"
                        Value="False">
                        <Setter Property="Opacity" Value="0.5"></Setter>
